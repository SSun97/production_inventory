{
  "name": "inventory_tracking",
  "version": "1.0.0",
  "description": "This is a simple web application for tracking inventory in a logistic company",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SSun97/production_inventory.git"
  },
  "author": "Simon Sun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SSun97/production_inventory/issues"
  },
  "homepage": "https://github.com/SSun97/production_inventory#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "json2csv": "^5.0.7",
    "morgan": "^1.10.0",
    "parcel-bundler": "^1.12.5",
    "pug": "^3.0.2",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2"
  }
}
